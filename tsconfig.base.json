{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES5",
    "lib": [
      "es2018",
      "esnext.asynciterable",
      "dom"
    ],
    "paths": {
      "ngx-pushview": [
        "dist/ngx-pushview/ngx-pushview",
        "dist/ngx-pushview"
      ],
      "ngx-db": [
        "dist/ngx-db/ngx-db",
        "dist/ngx-db"
      ],
      "ngx-push": [
        "dist/ngx-push/ngx-push",
        "dist/ngx-push"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
